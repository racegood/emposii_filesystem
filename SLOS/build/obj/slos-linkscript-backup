OUTPUT_FORMAT("elf32-littlearm", "elf32-littlearm", "elf32-littlearm")
OUTPUT_ARCH(arm)
ENTRY(_start)
SECTIONS
{
	. = 0xa00c0000;
	. = ALIGN(4);
	.text : { *(.text) }
	. = ALIGN(4);
	.rodata : { *(.rodata) }
	. = ALIGN(4);
	.data : { *(.data) }
	. = ALIGN(4);
	.got : { *(.got) }
	. = ALIGN(4);
	.bss : { *(.bss) }

	. = 0xa3900000;
	. = ALIGN(4);
	.ecache : {
		. = _ecache;
	   	*(.ecache) }

	. = 0xa4000000;
	. = ALIGN(4);
	.scache : {
		. = _scache;
	   	*(.scache) }
	

}
